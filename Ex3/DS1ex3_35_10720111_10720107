#include<iostream>

using namespace std ;

class Stack {
	
	
	
	public:
		
	void Push() ;
	void Pop() ;
	
	
    struct Data ;
	Data * top = NULL ;
	
	
}; // class Stack

struct Stack::Data {
  char ch ;
  Data * next = NULL ;
  Data * front = NULL ;
} ;

void Stack::Push( char ch ) {
  Data * temp ;
  temp = new Data ;
  temp->ch = ch ;
  if ( top != NULL ) {
    top->next = temp ;
    temp->front = top ;
    top = temp ;
  } // if
  else {
    top = temp ;
  } // else
  
} // Push()

void Stack::Pop() {
	top = top->front ;
	delete temp ;
	
} // Pop()

int main() {



  return 0 ;

} // main()
