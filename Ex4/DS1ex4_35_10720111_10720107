#include<iostream>
#include<vector>
#include<string>

using namespace std ;

struct Job {

    int ID ;
    int arrival ;
    int duration ;
    int timeout ;

};

class JobQueue {



    public:

    void Push() ;
    void Pop() ;
    void Front() ;
    void Back() ;

};

class JobList {


public:

    void ShellSort() ;
    bool Load() ;

};

void JobList::ShellSort(){



} // ShellSort

bool JobList::Load( string fileName ) {

    FILE *infile = NULL ;
    bool success = false ;

    fileName = "input" + fileName + ".txt" ;
    infile = fopen(fileName.c_str(), "r") ;

	if ( infile == NULL )
		;
	else{

        char temp ;

        while ( fscanf( infile, "%c", temp ) != EOF && temp != '\n' )
            ;

        vector<Job> jobList ;
        Job aJob ;
        while ( fscanf( infile, "%d %d %d %d", aJob.ID, aJob.arrival, aJob.duration, aJob.timeout ) != EOF )
            jobList.push_back( aJob ) ;

        success = true ;
        fclose( infile ) ;

	} // else

	return success ;

} // Load

int main(){



    return 0 ;

} // main()
